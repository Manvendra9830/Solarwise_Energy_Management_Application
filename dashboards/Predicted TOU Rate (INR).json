          },
          "format": "table",
          "hide": false,
          "range": true,
          "rawSql": "SELECT  \r\n    battery_level_kwh AS \"Battery Level (%)\",\r\n    tou_rates_inr AS \"TOU Rates (INR)\",\r\n    Time AS \"Timestamp\",\r\n    CASE \r\n        WHEN battery_level_kwh = 100 AND instantaneous_solar_generation_kwh > 0 \r\n        THEN instantaneous_solar_generation_kwh  -- Energy sold to grid\r\n        ELSE 0  -- No energy sold\r\n    END AS \"Energy Sold to Grid (kWh)\",\r\n    CASE \r\n        WHEN battery_level_kwh = 100 AND instantaneous_solar_generation_kwh > 0 \r\n        THEN instantaneous_solar_generation_kwh * tou_rates_inr  -- Calculate revenue\r\n        ELSE 0  -- No revenue if no energy sold\r\n    END AS \"Revenue from Grid (INR)\"\r\nFROM \r\n    solar_data9\r\nWHERE \r\n    Time = (SELECT MAX(Time) FROM solar_data9 WHERE Time <= CURRENT_TIMESTAMP)  -- Current time or last available time\r\nORDER BY \r\n    Time DESC  -- Sort by newest first\r\nLIMIT 1;\r\n",
          "refId": "A"
        }
      ],
      "title": "Energy Sold Back to Grid",
      "type": "stat"
    },
